    "start": "electron ."
  },
  "keywords": [
    "Electron",
    "BTS"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.1.0",
    "rimraf": "^3.0.2"
  }
}
