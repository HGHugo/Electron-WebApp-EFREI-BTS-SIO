{
  "name": "BTSSIOWebApp",
  "productName": "WebApp BTS SIO Efrei",
  "version": "1.0.0",
  "description": "A minimal app to display the BTS SIO Webapp",
  "author": "HGHugo",
  "main": "main.js",
  "scripts": {
    "build": "rimraf git-checker-* && electron-packager . --platform=darwin,linux,win32 --arch=x64 --icon=app --overwrite",
